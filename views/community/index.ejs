<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Community Playlists</title>
  </head>
  <body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>Community Playlists</h1>
        <div>
          <% playlists.forEach((list) => { %>
            <div>
                <a href="/community/<%= list._id %>">
                    <%= list.name %>
                </a> by <%= list.owner.username %>
            </div>        
          <% }) %>
        </div>

  </body>
</html>